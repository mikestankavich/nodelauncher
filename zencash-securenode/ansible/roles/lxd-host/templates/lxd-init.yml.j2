config:
  core.https_address: "[::]:{{ lxd_init_network_port }}"
  core.trust_password: "{{ lxd_init_trust_password }}"
networks:
- config:
    ipv4.address: auto
    ipv6.address: none
  description: ""
  name: lxdbr0
  type: bridge
  managed: true
storage_pools:
- config:
    source: lxd-pool
  description: ""
  name: default
  driver: zfs
profiles:
- config: {}
  description: ""
  devices:
    eth0:
      name: eth0
      network: lxdbr0
      type: nic
    root:
      path: /
      pool: default
      type: disk
  name: default
cluster: null
